{
  "name": "clay-smart-search",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "frontend": "npm run start --workspace=apps/frontend",
    "frontend build": "npm run build --workspace=apps/frontend",
    "backend": "npm run dev --workspace=apps/backend",
    "backend build": "npm run build --workspace=apps/backend",
    "dev": "npm-run-all --parallel frontend backend",
    "build": "npm run build --workspaces"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
