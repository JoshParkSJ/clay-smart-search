{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "outputDirectory": "dist",
  "buildCommand": "pnpm install && pnpm build",
  "installCommand": "npm i -g pnpm && pnpm install"
}